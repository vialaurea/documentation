msgid "Green = online"
msgstr "Verde = online"

#: ../../content/applications/productivity/discuss.rst:92
msgid "Orange = away"
msgstr "Laranja = ausente"

#: ../../content/applications/productivity/discuss.rst:93
msgid "White = offline"
msgstr "Branco = offline"

